<Window x:Class="Tour_Planner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tour_Planner"
        xmlns:views="clr-namespace:Tour_Planner.Views"
        xmlns:view_models="clr-namespace:Tour_Planner.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Content="{Binding DataContext, RelativeSource={RelativeSource Self}}">
    <Window.Resources>
        <DataTemplate
            DataType="{x:Type view_models:MainWindowViewModel}">
            <local:MainWindow/>
        </DataTemplate>
    </Window.Resources>
    <Window.DataContext>
        <view_models:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>

    </Grid>
</Window>
